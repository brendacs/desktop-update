{
  "name": "vue-desktop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "sh dist-commit.sh",
    "lint": "eslint src/**/*.{js,vue}",
    "format": "prettier src/**/*.{js,vue}",
    "clean": "rm -rf node_modules"
  },
  "dependencies": {
    "core-js": "^3.6.4",
    "jquery": "^3.5.0",
    "jquery.terminal": "^2.31.1",
    "vue": "^2.6.11",
    "vue-draggable-resizable": "^2.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-vue": "^7.0.0-alpha.0",
    "node-sass": "^4.14.0",
    "sass-loader": "^8.0.2",
    "url-loader": "^4.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
